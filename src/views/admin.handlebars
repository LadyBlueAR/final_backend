<div>
    <h1>Panel de Administrador</h1>
    <h1 style="text-align: center;">Lista De Usuarios</h1>
    <div>
 <ul>
        {{#each users}}
            <li>
                <h2>{{this.first_name}} {{this.last_name}}</h2>
                <p>Mail: {{this.email}}</p>
                <p>Rol: {{this.rol}}</p>
                <button type="button" userId="{{this._id}}" id="delete">Eliminar Usuario</button>  
                <button type="button" userId="{{this._id}}" id="changeRol">Modificar Rol</button>
            </li>
        {{/each}}
    </ul>
    </div>
</div>
<script src="/js/admin.js"></script>